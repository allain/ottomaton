{
  "name": "ottomaton",
  "version": "0.11.1",
  "description": "A toolkit for writing automation script DSLs influenced by Cucumber.",
  "main": "lib/index.js",
  "bin": {
    "otto": "./lib/bin/otto.js"
  },
  "scripts": {
    "compile": "babel --stage 2 --optional runtime --source-maps true -d lib/ src/ && chmod +x lib/bin/otto.js",
    "clean": "rm -r lib",
    "pretest": "npm run compile",
    "test": "blue-tape test/*.js | tap-dot",
    "prepublish": "npm run clean && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bmullan91/simple-factory"
  },
  "keywords": [
    "automation",
    "DSL"
  ],
  "author": "Allain Lalonde <allain.lalonde@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/allain/ottomaton/issues"
  },
  "homepage": "https://github.com/allain/ottomaton#readme",
  "dependencies": {
    "babel-runtime": "^5.8.25",
    "chalk": "^1.1.1",
    "debug": "^2.2.0",
    "defaults": "^1.0.3",
    "fj-flatten": "^1.0.0",
    "fj-map": "^1.0.1",
    "get-stdin": "^5.0.0",
    "is-promise": "^2.1.0",
    "map-in": "^1.0.2",
    "minimist": "^1.2.0",
    "native-promise-only": "^0.8.1",
    "promise-reduce": "^2.0.0",
    "simple-factory": "allain/simple-factory",
    "tap-dot": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "blue-tape": "^0.1.10",
    "tap-simple": "^0.3.1"
  }
}
