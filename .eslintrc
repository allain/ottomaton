{
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-console": [
      0
    ],
    "no-multiple-empty-lines": [
      2,
      {"max": 2}
    ],
    "strict": 0
  },
  "ecmaFeatures": {"modules": true},
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended"
}